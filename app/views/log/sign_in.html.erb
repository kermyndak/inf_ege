<div class="sign-in">
  <h1>Log In</h1>
  <%= form_with url: "/log/log", method: :post do |form| %>
    <div class="form-group">
      <%= form.label :email, "Email address:" %>
      <%= form.email_field :email, placeholder: "Enter email", class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.label :password, "Password:" %>
      <%= form.password_field :password, placeholder: 'password', class: "form-control" %>
    </div>
    <%= form.submit "Log In", class: "btn btn-success" %>
  <% end %>
  <%= button_to 'Homepage', root_path, method: :get, class: "btn btn-primary btn-sign-in" %>

  <div class="errors-class">
    <div id="msg"></div>
  </div>
</div>